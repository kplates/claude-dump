{
  "name": "claude-dummp",
  "version": "0.1.0",
  "description": "Real-time web viewer for Claude Code conversation history",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "claude",
    "claude-code",
    "conversation",
    "viewer",
    "ai"
  ],
  "bin": {
    "claude-dummp": "./dist/server/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node dist/server/index.js",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "chokidar": "^4.0.0",
    "diff": "^7.0.0",
    "express": "^4.21.0",
    "open": "^10.1.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/diff": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/ws": "^8.5.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.0",
    "diff2html": "^3.4.0",
    "highlight.js": "^11.10.0",
    "lucide-react": "^0.468.0",
    "postcss": "^8.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.0",
    "rehype-highlight": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0"
  }
}
