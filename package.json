{
  "name": "claude-dump",
  "version": "0.3.1",
  "description": "Real-time web viewer for Claude Code conversation history",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "claude",
    "claude-code",
    "conversation",
    "viewer",
    "ai"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kplates/claude-dump.git"
  },
  "homepage": "https://github.com/kplates/claude-dump",
  "bugs": "https://github.com/kplates/claude-dump/issues",
  "bin": {
    "claude-dump": "./dist/server/server/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "NODE_ENV=development tsx src/server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node dist/server/server/index.js",
    "prepublishOnly": "npm run build",
    "release": "npm publish",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish"
  },
  "dependencies": {
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/xterm": "^6.0.0",
    "chokidar": "^4.0.0",
    "diff": "^7.0.0",
    "express": "^4.21.0",
    "node-pty": "^1.2.0-beta.10",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/diff": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/ws": "^8.5.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.0",
    "diff2html": "^3.4.0",
    "highlight.js": "^11.10.0",
    "lucide-react": "^0.468.0",
    "postcss": "^8.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.0",
    "rehype-highlight": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0"
  }
}
